{
  "name": "WYSIWYG",
  "version": "0.0.1",
  "main": "private",
  "scripts": {
    "dev": "NODE_ENV=development webpack server --config config/webpack.dev.config.js",
    "build": "NODE_ENV=production webpack --config config/webpack.prod.config.js",
    "lint": "eslint --fix --ext .ts,.vue src",
    "lint:fix": "eslint --fix src/App.vue",
    "prepare": "husky install",
    "commit": "git-cz",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "keywords": [],
  "author": "lvfangren",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "axios": "0.21.1",
    "normalize.css": "8.0.1",
    "precss": "4.0.0",
    "vue": "2.6.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.14.7",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.3.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "css-loader": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.14.0",
    "eslint-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.1.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "vue-eslint-parser": "^7.9.0",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.44.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 12.13.0",
    "npm": ">= 6.12.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E $HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:fix"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint",
      "git add"
    ]
  }
}
